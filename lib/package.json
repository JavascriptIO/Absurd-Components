{
  "name": "angular-absurd-components",
  "version": "0.1.0",
  "scripts": {
    "build": "gulp build",
    "postbuild": "cd ../dist && npm link",
    "build:watch": "gulp",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p tsconfig.json -n angular-absurd-components -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "lint": "tslint --type-check --project tsconfig.json src/**/*.ts",
    "test": "tsc && karma start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JavascriptIO/Absurd-Components"
  },
  "author": "JavaScript.io (see contributors)",
  "keywords": [
    "angular",
    "absurd",
    "components"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JavascriptIO/Absurd-Components/issues"
  },
  "devDependencies": {
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@compodoc/compodoc": "^1.0.0-beta.7",
    "@types/jasmine": "2.5.52",
    "@types/node": "~8.0.1",
    "codelyzer": "~3.1.1",
    "core-js": "^2.4.1",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "gulp-rollup": "^2.11.0",
    "jasmine-core": "~2.6.4",
    "jasmine-spec-reporter": "~4.1.1",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "node-sass": "^4.5.2",
    "node-sass-tilde-importer": "^1.0.0",
    "node-watch": "^0.5.2",
    "protractor": "~5.1.0",
    "rollup": "^0.43.0",
    "run-sequence": "^1.2.2",
    "rxjs": "^5.1.0",
    "ts-node": "~3.1.0",
    "tslint": "~5.4.3",
    "typescript": "~2.3.4",
    "zone.js": "^0.8.4"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "animejs": "^2.0.2"
  }
}
